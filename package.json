{
  "name": "gerrit-vscode",
  "version": "0.5.0",
  "displayName": "Gerrit",
  "description": "A Gerrit extension for vscode",
  "authors": [
    "Thomas Haakon Townsend"
  ],
  "publisher": "tht13",
  "repository": {
    "type": "git",
    "url": "https://github.com/tht13/gerrit-vscode.git"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.46.0"
  },
  "main": "./dist/extension.js",
  "icon": "images/gerrit_icon.png",
  "galleryBanner": {
    "color": "#eeeeee",
    "theme": "light"
  },
  "bugs": "https://github.com/tht13/gerrit-vscode/issues",
  "homepage": "https://github.com/tht13/gerrit-vscode/blob/master/README.md",
  "categories": [
    "SCM Providers"
  ],
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Gerrit",
      "properties": {
        "gerrit.active": {
          "type": "boolean",
          "default": false,
          "description": "Activate Gerrit extension"
        },
        "gerrit.url": {
          "type": "string",
          "examples": [
            "https://review.gerrithub.io",
            "http://localhost:8080"
          ],
          "format": "uri",
          "description": "The Gerrit URL (required)"
        },
        "gerrit.project": {
          "type": "string",
          "description": "Gerrit project (required)"
        },
        "gerrit.username": {
          "type": "string",
          "scope": "application",
          "description": "Gerrit username (required)"
        },
        "gerrit.httpPassword": {
          "type": "string",
          "scope": "application",
          "description": "Gerrit http password (required)"
        },
        "gerrit.showLog": {
          "type": "boolean",
          "default": false,
          "description": "Show the gerrit log during commands"
        }
      }
    },
    "commands": [
      {
        "command": "gerrit.stageAll",
        "title": "Stage all files",
        "category": "Gerrit"
      },
      {
        "command": "gerrit.stageCurrentFile",
        "title": "Stage current file",
        "category": "Gerrit"
      },
      {
        "command": "gerrit.stageFile",
        "title": "Stage file",
        "category": "Gerrit"
      },
      {
        "command": "gerrit.resetAll",
        "title": "Reset all files",
        "category": "Gerrit"
      },
      {
        "command": "gerrit.resetCurrentFile",
        "title": "Reset current file",
        "category": "Gerrit"
      },
      {
        "command": "gerrit.resetFile",
        "title": "Reset file",
        "category": "Gerrit"
      },
      {
        "command": "gerrit.checkoutBranch",
        "title": "Checkout branch",
        "category": "Gerrit"
      },
      {
        "command": "gerrit.checkoutRevision",
        "title": "Checkout revision",
        "category": "Gerrit"
      },
      {
        "command": "gerrit.cherrypickRevision",
        "title": "Cherrypick revision",
        "category": "Gerrit"
      },
      {
        "command": "gerrit.cherrypickContinue",
        "title": "Cherrypick continue",
        "category": "Gerrit"
      },
      {
        "command": "gerrit.commit",
        "title": "Commit",
        "category": "Gerrit"
      },
      {
        "command": "gerrit.commitAmend",
        "title": "Commit amend",
        "category": "Gerrit"
      },
      {
        "command": "gerrit.pushBranch",
        "title": "Push to branch",
        "category": "Gerrit"
      },
      {
        "command": "gerrit.draftBranch",
        "title": "Draft to branch",
        "category": "Gerrit"
      },
      {
        "command": "gerrit.rebaseBranch",
        "title": "Rebase from branch",
        "category": "Gerrit"
      },
      {
        "command": "gerrit.rebaseContinue",
        "title": "Rebase continue",
        "category": "Gerrit"
      },
      {
        "command": "gerrit.log",
        "title": "Toggle log",
        "category": "Gerrit"
      },
      {
        "command": "gerrit.cleanAll",
        "title": "Clean all files",
        "category": "Gerrit"
      },
      {
        "command": "gerrit.cleanCurrentFile",
        "title": "Clean current file",
        "category": "Gerrit"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "npm run test-compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/lodash": "^4.14.168",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/object-hash": "^1.3.4",
    "@types/request-promise": "^4.1.47",
    "@types/vscode": "^1.46.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "ts-loader": "^8.0.14",
    "typescript": "^4.2.3",
    "vsce": "^1.87.0",
    "vscode-test": "^1.5.0",
    "webpack": "^5.19.0",
    "webpack-cli": "^4.4.0"
  },
  "dependencies": {
    "lodash": "^4.15.0",
    "object-hash": "^2.1.1",
    "request-promise": "^4.2.6",
    "vscode-languageclient": "^7.0.0",
    "vscode-languageserver": "^7.0.0"
  },
  "volta": {
    "node": "12.21.0",
    "npm": "7.7.6"
  }
}
